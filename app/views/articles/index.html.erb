<div class="container">
    <div class="identical">
      All Boards
    </div>
  </div>

  <% @articles.each do |article|%>
    <%= link_to article_path(article) do %>
      <div class="card">
        <div class="card_image">
          <%= image_tag 'Photo.png' %>
        </div>
        <div class="card_content">
        <div class="card_title">
        <%= article.title %>
        </div>
          <div class="card_heart">
          </div>
          <div class="card_detail">
            <%= image_tag 'Oval2.png' %>
            <div>
            </div>
          </div>
        </div>
      </div>
    <%end%>
  <%end%>

  <%= link_to new_article_path	do %>
    <div class="float-btn">
      <div class="add">
        + Add new card
      </div>
    </div>
  <%end%>

</div>